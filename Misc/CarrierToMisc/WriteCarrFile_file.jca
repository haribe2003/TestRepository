<adapter-config name="WriteCarrFile" adapter="File Adapter" wsdlLocation="WriteCarrFile.wsdl" xmlns="http://platform.integration.oracle/blocks/adapter/fw/metadata">
  
  <connection-factory location="eis/FileAdapter"/>
  <endpoint-interaction portType="Write_ptt" operation="Write">
    <interaction-spec className="oracle.tip.adapter.file.outbound.FileInteractionSpec">
      <property name="LogicalDirectory" value="writeFile"/>
      <property name="Append" value="true"/>
      <property name="FileNamingConvention" value="CARR_*.txt"/>
    </interaction-spec>
  </endpoint-interaction>

</adapter-config>